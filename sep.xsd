            </xs:annotation>
          </xs:element>
          <xs:element name="ifPromiscuousMode" minOccurs="0" maxOccurs="1" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Use rules from [RFC 2863].</xs:documentation>
            </xs:annotation>
          </xs:element>
