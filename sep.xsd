            </xs:annotation>
          </xs:element>
          <xs:element name="ifType" minOccurs="0" maxOccurs="1" type="UInt16">
            <xs:annotation>
              <xs:documentation>Use rules and assignments from [RFC 2863].</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IPAddrListLink" type="IPAddrListLink" minOccurs="0" maxOccurs="1" />
          <xs:element name="lastResetTime" minOccurs="0" maxOccurs="1" type="Int64">
            <xs:annotation>
              <xs:documentation>Similar to ifLastChange in [RFC 2863].</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="lastUpdatedTime" minOccurs="0" maxOccurs="1" type="Int64">
            <xs:annotation>
              <xs:documentation>The date/time of the reported status.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LLInterfaceListLink" type="LLInterfaceListLink" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
